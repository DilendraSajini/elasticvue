    "@tauri-apps/api": "^1.6.0",
    "codemirror": "^6.0.1",
    "idb": "^8.0.0",
    "json-bigint": "^1.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "pretty-bytes": "^6.1.1",
    "quasar": "^2.16.6",
    "vue": "^3.4.31",
    "vue-i18n": "^9.13.1",
    "vue-resizable": "^2.1.7",
