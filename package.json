    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.7.0",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.26"
